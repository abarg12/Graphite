; ModuleID = 'Graphite'
source_filename = "Graphite"

%node_t = type { i8*, i1, i8* }

@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %n = alloca %node_t
  %temp = getelementptr inbounds %node_t, %node_t* %n, i32 0, i32 1
  store i1 true, i1* %temp
  %temp1 = getelementptr inbounds %node_t, %node_t* %n, i32 0, i32 1
  %b = load i1, i1* %temp1
  %b2 = alloca i1
  store i1 %b, i1* %b2
  %b3 = load i1, i1* %b2
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @fmt, i32 0, i32 0), i1 %b3)
  ret i32 0
}
